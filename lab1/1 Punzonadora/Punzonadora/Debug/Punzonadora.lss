                                 SEND_MESSAGE:
00022a 930f                      	push r16
                                 
                                 	SEND_MESSAGE_LOOP:
00022b 9105                      	lpm r16, Z+
00022c 3000                      	cpi r16, 0
                                 
00022d f011                      	breq SEND_MESSAGE_END
00022e dfd6                      	rcall USART_WRITE_BYTE
00022f cffb                      	rjmp SEND_MESSAGE_LOOP
                                 
                                 	SEND_MESSAGE_END:
000230 910f                      	pop r16
000231 9508                      	ret
                                 
                                 
                                 
                                 
