                                 USART_INIT:		
                                 	; Set baud rate
0001fa 9310 00c5                 	sts UBRR0H, r17
0001fc 9300 00c4                 	sts UBRR0L, r16
                                 	; Enable receiver and transmitter, and interruptions
0001fe e908                      	ldi r16, (1<<RXEN0)|(1<<TXEN0)|(1<<RXCIE0)
0001ff 9300 00c1                 	sts UCSR0B,r16
                                 	; Set frame format: 8data, 1stop bit
000201 e006                      	ldi r16, (0<<USBS0)|(3<<UCSZ00)
000202 9300 00c2                 	sts UCSR0C, r16
000204 9508                      	ret
                                 	
                                 
                                 ; write r16 to usart
                                 USART_WRITE_BYTE:
000205 931f                          push r17
000206 932f                          push r18
000207 933f                          push r19
000208 93ff                          push ZH
000209 93ef                          push ZL
                                 
                                     ; head/tail
00020a 9110 0200                     lds  r17, tx_head
00020c 9120 0201                     lds  r18, tx_tail
                                 
                                     ; next = (head + 1) & MASK   (keep it in r19!)
00020e 2f31                          mov  r19, r17				; r17 holds the prev. value of tx_head
00020f 9533                          inc  r19					; 
000210 7f3f                          andi r19, TX_BUF_MASK		; this is a clever reset function!
                                 								; Clamps the value between 0 and the TX_BUF_MASK
                                 
                                 wait_space:
                                     ; Active wait when buffer is full until space is 
000211 9120 0201                     lds r18, tx_tail
000213 1732                          cp r19, r18
000214 f3e1                          breq wait_space
000215 c000                      	rjmp have_space
                                 
                                 have_space:
                                     ; Z = &tx_buffer[head]
000216 e0e0                          ldi  ZL, low(tx_buffer)
000217 e0f1                          ldi  ZH, high(tx_buffer)
000218 0fe1                          add  ZL, r17
000219 1df1                          adc  ZH, r1            ; r1 must be 0
                                 
00021a 8300                          st   Z, r16            ; write byte
                                 
                                     ; head = next
00021b 9330 0200                     sts  tx_head, r19
                                 
                                     ; enable UDRE interrupt so ISR starts/continues draining
00021d 94f8                          cli
00021e 9120 00c1                     lds  r18, UCSR0B
000220 6220                          ori  r18, (1<<UDRIE0)
000221 9320 00c1                     sts  UCSR0B, r18
000223 9478                          sei
                                 
000224 91ef                          pop  ZL
000225 91ff                          pop  ZH
000226 913f                          pop  r19
000227 912f                          pop  r18
000228 911f                          pop  r17
000229 9508                          ret
                                 
