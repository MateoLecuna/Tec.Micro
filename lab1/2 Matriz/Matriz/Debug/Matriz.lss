                                 	; IO configuration
00010e e30f
00010f b904                      	ldi r16, 0b00111111 out DDRB,  r16 ; 
000110 e30f
000111 b907                      	ldi r16, 0b00111111 out DDRC,  r16 ; 
000112 ef00
