                                 	; Enable receiver and transmitter, and interruptions
000169 e908                      	ldi r16, (1<<RXEN0)|(1<<TXEN0)|(1<<RXCIE0)
00016a 9300 00c1                 	sts UCSR0B,r16
                                 	; Set frame format: 8data, 2stop bit
00016c e00e                      	ldi r16, (1<<USBS0)|(3<<UCSZ00)
