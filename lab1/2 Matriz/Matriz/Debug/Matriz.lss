000171 931f                      	push r17
000172 932f                      	push r18
000173 933f                      	push r19
000174 934f                      	push r20
000175 93ef                      	push ZL
000176 93ff                      	push ZH
                                 
                                     ; load head and tail
000177 9110 0200                     lds  r17, tx_head               ; r17 = head
000179 9120 0201                     lds  r18, tx_tail               ; r18 = tail
                                 
