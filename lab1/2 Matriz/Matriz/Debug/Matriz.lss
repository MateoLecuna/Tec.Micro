0001ed 1712                          cp   r17, r18
0001ee f431                          brne usart_udre_send
                                     ; empty ? disable UDRE interrupt
0001ef 9140 00c1                     lds  r20, UCSR0B
0001f1 7d4f                          andi r20, ~(1<<UDRIE0)
0001f2 9340 00c1                     sts  UCSR0B, r20
