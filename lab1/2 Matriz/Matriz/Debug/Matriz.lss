00016f 9508                      	ret
                                 
                                 
                                 USART_SEND:
                                     ; load head and tail
000170 9110 0110                     lds  r17, tx_head               ; r17 = head
000172 9120 0111                     lds  r18, tx_tail               ; r18 = tail
                                 
                                     ; next_head = (head + 1) & MASK
000174 2f31                          mov  r19, r17
000175 9533                          inc  r19
000176 703f                          andi r19, TX_BUF_MASK
                                 
                                     ; if next_head == tail => buffer full
000177 1732                          cp   r19, r18
